{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", 4],
        "no-console": "off",
        "no-use-before-define": ["error", { "functions": false, "classes": false }],
        "no-return-assign" : 0
    }
}
